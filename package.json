{
  "name": "webpack-developer-kit",
  "version": "1.0.0",
  "description": "Super Light Dev Kit for Webpack Developers",
  "main": "",
  "scripts": {
    "dev": "webpack-dev-server --inline --content-base build/",
    "build": "rimraf ./dist && webpack --config webpack.config.js",
    "lint": "eslint .",
    "test:unit": "jest",
    "test:e2e": "jasmine e2e.spec.js",
    "debug": "node --inspect --debug-brk ./node_modules/webpack/bin/webpack.js --config webpack.config.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blacksonic/babel-webpack-tree-shaking.git"
  },
  "keywords": [
    "tree-shaking",
    "babel",
    "webpack",
    "toolkit",
    "developer"
  ],
  "author": {
    "name": "Sean Larkin",
    "email": "sean.larkin@cuw.edu"
  },
  "license": "MIT",
  "jest": {
    "rootDir": "./app/"
  },
  "dependencies": {
    "acorn": "^4.0.3",
    "debug": "^2.2.0",
    "loader-utils": "^0.2.16",
    "source-map": "^0.5.6"
  },
  "devDependencies": {
    "@types/acorn": "^1.0.31",
    "@types/debug": "^0.0.29",
    "@types/node": "^6.0.46",
    "@types/webpack": "^1.12.35",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "babili": "^0.0.7",
    "babili-webpack-plugin": "^0.0.6",
    "cheerio": "0.22.0",
    "chromedriver": "2.24.1",
    "enhanced-resolve": "^2.3.0",
    "eslint": "^3.9.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.0",
    "http-server": "^0.9.0",
    "jasmine": "2.5.2",
    "jest": "16.0.2",
    "rimraf": "^2.5.4",
    "selenium-webdriver": "3.0.0-beta-3",
    "ts-node": "^1.6.1",
    "typescript": "^2.0.6",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-sources": "^0.1.2"
  }
